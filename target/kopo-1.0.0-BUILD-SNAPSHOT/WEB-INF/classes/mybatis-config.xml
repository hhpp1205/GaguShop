<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="kr.ac.kopo.model.Member" alias="Member" />
		<typeAlias type="kr.ac.kopo.model.Gagu" alias="Gagu" />
		<typeAlias type="kr.ac.kopo.model.GaguKeyword" alias="GaguKeyword" />
		<typeAlias type="kr.ac.kopo.model.Attach" alias="Attach" />
		<typeAlias type="kr.ac.kopo.model.Orders" alias="Orders" />
		<typeAlias type="kr.ac.kopo.model.Wish" alias="Wish" />
		<typeAlias type="kr.ac.kopo.model.Review" alias="Review" />
		<typeAlias type="kr.ac.kopo.model.AdminTotal" alias="AdminTotal" />
	</typeAliases>
	
	<mappers>
		<mapper resource="mybatis/member.xml" />
		<mapper resource="mybatis/gagu.xml" />
		<mapper resource="mybatis/cart.xml" />
		<mapper resource="mybatis/attach.xml" />
		<mapper resource="mybatis/orders.xml" />
		<mapper resource="mybatis/wish.xml" />
		<mapper resource="mybatis/review.xml" />
	</mappers>
</configuration>